        <rdf:type rdf:resource="http://www.semanticweb.org/nlp-wiki.owl#Method"/>
        <subFieldOf rdf:resource="http://www.semanticweb.org/nlp-wiki.owl#Attention"/>
        <subFieldOf rdf:resource="http://www.semanticweb.org/nlp-wiki.owl#Positional_Encoding"/>
