    <!-- http://www.semanticweb.org/nlp-wiki.owl#NDCG@k -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/nlp-wiki.owl#NDCG@k">
        <rdf:type rdf:resource="http://www.semanticweb.org/nlp-wiki.owl#FieldOfStudy"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/nlp-wiki.owl#Metric"/>
